{
  "version": 2,
  "builds": [
    {
      "src": "pilgrimage/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "wsgi": {
          "app": "pilgrimage.wsgi:application",
          "loader": "python"
        }
      }
    },
    {
      "src": "staticfiles/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/.*",
      "dest": "pilgrimage/wsgi.py"
    }
  ]
}